    <hudson.tasks.Shell>
      <command>#!/bin/bash -xe



BRANCH=`git branch --no-color -r --contains HEAD | grep -v -e &apos;-&gt;&apos; | head -1 | awk -F&apos;/&apos; &apos;{print $NF}&apos;`
NEWTAG=`echo $GERRIT_REFNAME | awk -F/ &apos;{print $NF}&apos;`
VersionName=$(echo $NEWTAG | cut -d&apos;_&apos; -f1)
#replace the VersionName in package json
sed  -ri &quot;s/\&quot;version\&quot;:.*/\&quot;version\&quot;: \&quot;${VersionName}\&quot;,/g&quot; package.json
cat package.json
NAME=$(cat package.json | grep &apos;name&apos; | cut -d&apos;:&apos; -f2  | tr -d &apos;,&apos;&apos; &apos;&apos;&quot;&apos;)
ZIP_NAME=&quot;${NAME}-v${NEWTAG}.zip&quot;

npm install
npm run build

zip -r $ZIP_NAME ./build ./run-server.bat ./server.js


echo &quot;NAME=${NAME}&quot; &gt; prop
echo &quot;NEWTAG=${NEWTAG}&quot; &gt;&gt; prop
echo &quot;BRANCH=${BRANCH}&quot; &gt;&gt; prop</command>
    </hudson.tasks.Shell>
