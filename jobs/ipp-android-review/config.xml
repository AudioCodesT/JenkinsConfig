    <hudson.tasks.Shell>
      <command>#!/bin/bash -x
#the above line is needed because we are going to use &quot;source&quot; command

rm -f *.zip
repo init -u ssh://jenkins@gerrit:29418/IPP/Android/repo_manifests -b $GERRIT_BRANCH

# second need to sync on latest changes off all other repositories
echo ----- repo sync -c --no-clone-bundle -j16 --force-sync
repo sync -c --no-tags --no-clone-bundle -j16 --force-sync

# cd to releant path
echo Fetch this change $GERRIT_REFSPEC
cd $(repo list | grep $GERRIT_PROJECT$ | cut -d: -f1)
pwd
echo ----- Fetch $GERRIT_PATCHSET_REVISION in branch $GERRIT_BRANCH
echo ----- Change $GERRIT_REFSPEC
git log -2 --oneline
echo git fetch ssh://jenkins@gerrit:29418/$GERRIT_PROJECT $GERRIT_REFSPEC
git fetch ssh://jenkins@gerrit:29418/$GERRIT_PROJECT $GERRIT_REFSPEC &amp;&amp; git checkout FETCH_HEAD
echo ----- git log after fetch
git log -2 --oneline

cd $WORKSPACE

###  adding temporary condition for Oriel gerri tchange:
if [[ &quot;$GERRIT_CHANGE_NUMBER&quot; == &quot;24262&quot; ]]
then	./build/tools/make_c450hd.sh --zip-images --clean
else	./build/tools/make_c450hd.sh --zip-images
fi</command>
    </hudson.tasks.Shell>
