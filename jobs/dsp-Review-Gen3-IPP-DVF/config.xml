    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}
set -xe

#if it&apos;s repo_manifests project then repo init the change, else repo init the branch
if [[ &quot;${GERRIT_PROJECT}&quot; == &quot;DSP/Gen3/IPP/DVF&quot; ]]
then
	what_to_build=${GERRIT_BRANCH}
elif [[ &quot;${GERRIT_PROJECT}&quot; == &quot;DSP/Gen3/IPP/DVF/repo_manifests&quot; ]]
then
	what_to_build=${GERRIT_REFSPEC}
fi

echo &quot;repo init -u ssh://jenkins@gerrit:29418/DSP/Gen3/IPP/DVF/repo_manifests -b ${what_to_build}&quot;
repo init -u ssh://jenkins@gerrit:29418/DSP/Gen3/IPP/DVF/repo_manifests -b ${what_to_build}
echo &quot;repo sync -j$(nproc) --force-sync&quot;
repo sync -j$(nproc) --force-sync

# cd to releant path
cd DVF
if [[ &quot;${GERRIT_PROJECT}&quot; == &quot;DSP/Gen3/IPP/DVF&quot; ]]
then
	echo &quot;git fetch ssh://jenkins@gerrit:29418/DSP/Gen3/IPP/DVF $GERRIT_REFSPEC&quot;
	git fetch ssh://jenkins@gerrit:29418/DSP/Gen3/IPP/DVF $GERRIT_REFSPEC &amp;&amp; git checkout FETCH_HEAD
fi

#copy all modules to relevant path in Gen5 Modules directory
echo &quot;sh copyModules.sh&quot;
sh copyModules.sh

chmod -R 775 ${WORKSPACE}/DVF/delivery

if [[ &quot;$GERRIT_BRANCH&quot; == &quot;SDK135&quot; ]]
then
/usr/local/DS-5_v5.26.2/bin/suite_exec --toolchain &quot;ARM Compiler 5 (DS-5 built-in)&quot; bash --norc &lt;&lt; EOF
cd ${WORKSPACE}/DVF/delivery/build
rm -rf output
make clean
make config_evb99
make
make headers
make app_libs
EOF
elif [[ &quot;$GERRIT_BRANCH&quot; == &quot;SDK240&quot; ]]
then
/usr/local/DS-5_v5.26.2/bin/suite_exec --toolchain &quot;ARM Compiler 5 (DS-5 built-in)&quot; bash --norc &lt;&lt; EOF
cd ${WORKSPACE}/DVF/delivery/build
rm -rf output
make clean
make config_evb101
make
make headers
make app_libs
EOF
elif [[ &quot;$GERRIT_BRANCH&quot; == &quot;SDK280&quot; ]]
then
/usr/local/DS-5_v5.26.2/bin/suite_exec --toolchain &quot;ARM Compiler 5 (DS-5 built-in)&quot; bash --norc &lt;&lt; EOF
cd ${WORKSPACE}/DVF/delivery/build
rm -rf output
make clean
make config_evb99_split
make
make headers
make app_libs
EOF
fi
</command>
    </hudson.tasks.Shell>
