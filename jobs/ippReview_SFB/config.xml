${WORKSPACE}/build/image/phone.bin=&gt;SFB/$AUTOPRD/build-${BUILD_NUMBER}/gerrit-change-${GERRIT_CHANGE_NUMBER}_${GERRIT_PATCHSET_NUMBER}
