  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash -xe

#get Last Tag and all the commits since the last tag in reverse list
OLDTAG=$(git describe --tags --abbrev=0 --first-parent)
echo $OLDTAG
NEWTAG=`echo $OLDTAG | sed -r 's/(.*)\.[^\d]+$/\1/'`.`echo $OLDTAG | awk -F. '{print $NF+1}'`
echo $NEWTAG

#if VersionName is not empty then push VersionName as the new Tag
if [ -n "$VersionName" ]
then
	echo "Version Name entered by the User"
    if [[ $VersionName =~ ^[0-9]+.[0-9]+.[0-9]+$ ]]
    then
    	NEWTAG=${VersionName}
    else
    	echo "The VersionName parameter need to contain only version name, for example: 1.1.3"
        exit 1
    fi
fi
echo NEWTAG=$NEWTAG &gt; prop

npm config set registry https://artifactory/api/npm/npm/
npm config set strict-ssl false
sudo npm i -g npm@4.6.1
#change package.json to the correct version
sed  -ri "s/\"version\":.*/\"version\": \"${NEWTAG}\",/g" package.json
#run npm install
npm install
npm pack
sudo npm i -g npm@6.13.4

</command>
    </hudson.tasks.Shell>
