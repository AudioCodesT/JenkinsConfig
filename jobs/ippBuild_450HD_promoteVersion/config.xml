    <hudson.tasks.Shell>
      <command># Update file with new version num
IPP_VER_PREV=`grep IP_Phone_ACL_VERSION include/ipp_version.h_shipped | sed -r &apos;s/.*\s+&quot;([0-9.]+)&quot;/\1/&apos;`
IPP_MAJOR_VER=`echo ${IPP_VER_PREV} | cut -d. -f1,2,3`
echo Last tag: ${IPP_VER_PREV} &gt; log-info.txt
#fourth=`grep IP_Phone_ACL_VERSION include/ipp_version.h_shipped | sed -r &apos;s/.*&quot;.*\.([0-9]+)&quot;/\1/&apos;`
fourth=`echo ${IPP_VER_PREV} | sed -r &apos;s/.*\.([0-9]+)/\1/&apos;`
((fourth++))
sed -r &quot;s/(IP_Phone_ACL_VERSION\s+\&quot;.*\.)[0-9]+\&quot;/\1$fourth\&quot;/&quot; include/ipp_version.h_shipped &gt; new_ipp_version.h_shipped
mv -f new_ipp_version.h_shipped include/ipp_version.h_shipped</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command># Get majoe and monir versions
IPP_VER=`grep IP_Phone_ACL_VERSION include/ipp_version.h_shipped | sed -r &apos;s/.*\s+&quot;([0-9.]+)&quot;/\1/&apos;`
if [ `git tag -l $IPP_VER` ]; then git tag -d $IPP_VER; fi
echo IPP_VER=${IPP_VER} &gt; generated_ipp_version.properties
</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@1.92.1">
      <info>
        <propertiesFilePath>generated_ipp_version.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.Shell>
      <command>echo ipp version is: ${IPP_VER}</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command># Commit conf file with new version
git config --global user.name &quot;Jenkins&quot;
git config --global user.email devops@audiocodes.com
message=&quot;promote version to ${IPP_VER}&quot;
git commit -am &quot;$message&quot;
git push origin 2.0.13_450HD
#git push origin ${IPP_VER}
</command>
