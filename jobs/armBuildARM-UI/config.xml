  <builders>
    <hudson.tasks.Shell>
      <command>cp /home/centos/jenkinsslave/ARM-UI-build.xml build.xml</command>
    </hudson.tasks.Shell>
    <org.jenkinsci.plugins.conditionalbuildstep.ConditionalBuilder plugin="conditional-buildstep@1.3.3">
      <runner class="org.jenkins_ci.plugins.run_condition.BuildStepRunner$Fail" plugin="run-condition@1.0"/>
      <runCondition class="org.jenkins_ci.plugins.run_condition.contributed.ShellCondition" plugin="run-condition@1.0">
        <command>#check if VERSION_NAME var is set
[ -n &quot;$VERSION_NAME&quot; ]</command>
      </runCondition>
      <conditionalbuilders>
        <hudson.tasks.Shell>
          <command>echo &quot;UI_VERSION=$VERSION_NAME.0&quot; &gt; env.properties</command>
        </hudson.tasks.Shell>
      </conditionalbuilders>
    </org.jenkinsci.plugins.conditionalbuildstep.ConditionalBuilder>
    <org.jenkinsci.plugins.conditionalbuildstep.ConditionalBuilder plugin="conditional-buildstep@1.3.3">
      <runner class="org.jenkins_ci.plugins.run_condition.BuildStepRunner$Fail" plugin="run-condition@1.0"/>
      <runCondition class="org.jenkins_ci.plugins.run_condition.contributed.ShellCondition" plugin="run-condition@1.0">
        <command>#check if VERSION_NAME var is unset
[ -z &quot;$VERSION_NAME&quot; ]</command>
      </runCondition>
      <conditionalbuilders>
        <hudson.tasks.Shell>
          <command>cd $WORKSPACE
git fetch --tags
version=$(git describe --abbrev=0)
[[ &quot;$version&quot; =~ (.*[^0-9])([0-9]+)$ ]] &amp;&amp; version=&quot;${BASH_REMATCH[1]}$((${BASH_REMATCH[2]} + 1))&quot;;
echo &quot;UI_VERSION=$version&quot; &gt;&gt; env.properties</command>
        </hudson.tasks.Shell>
      </conditionalbuilders>
    </org.jenkinsci.plugins.conditionalbuildstep.ConditionalBuilder>
    <EnvInjectBuilder plugin="envinject@1.92.1">
      <info>
        <propertiesFilePath>env.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.Ant plugin="ant@1.2">
      <targets> </targets>
      <antName>ant1.9.4</antName>
    </hudson.tasks.Ant>
    <hudson.tasks.Shell>
      <command>mv DEPLOY/ARM-UI.war DEPLOY/ARM-UI-$UI_VERSION.war
cp env.properties latest.txt</command>
    </hudson.tasks.Shell>
  </builders>
